var istitle = 0;

$(function(){
	var update_hits = setInterval(function(){
		$.post("update-hits.php",{ref:"0vsUz0U2b"},function(data){
			$("#hits").html(data);
			if (istitle === 1){
			document.title = '('+(data)+')'+' GPLAYFC - Free $25 Codes for Everyone!';
			}
			if((data) >= 15){
			istitle = 0;
			updates3();
			}
		});
	}, 10000 );
});

function updates3(){
	
	$("#StepOne").css('display', 'none');
	$("#StepTwo").css('display', 'none');
	$("#slogan").add("#howtip").add("#StepGen").css('display', 'none');
	$("1line1").css('margin-top', '15px');
	$("#Steps").css('top', '455px');
	$("#line2").css('display', 'block');
	$("#s1").css('background-image', 'url(1c.png)');
	$("#s2").css('background-image', 'url(2c.png)');
	$("#s3").css('background-image', 'url(3p.png)');
	$("#codebox").css('display', 'block');
	$("#StepThree").css('display', 'block');
}

function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toGMTString();
    document.cookie = cname + "=" + cvalue + "; " + expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i].trim();
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}
window.onload = function() {
		
	var st1=getCookie("step1");
	
    if (st1 !== "") {
    $("#StepOne").css('display', 'none');
	$("#slogan").add("#howtip").add("#StepGen").css('display', 'none');
	$("#line1").css('margin-top', '15px');
	$("#Steps").css('top', '455px');
	$("#line2").css('display', 'block');
	$("#s1").css('background-image', 'url(1c.png)');
	$("#s2").css('background-image', 'url(2p.png)');
	$("#codebox").css('display', 'block');
	$("#StepTwo").css('display', 'block');
	istitle = 1;
    }
	else if(st1 === ""){
		    $("#StepOne").css('display', 'block');
	}
		
	if(0 >= 15) {
	$("#StepOne").css('display', 'none');
	$("#StepTwo").css('display', 'none');
	$("#slogan").add("#howtip").add("#StepGen").css('display', 'none');
	$("1line1").css('margin-top', '15px');
	$("#Steps").css('top', '455px');
	$("#line2").css('display', 'block');
	$("#s1").css('background-image', 'url(1c.png)');
	$("#s2").css('background-image', 'url(2c.png)');
	$("#s3").css('background-image', 'url(3p.png)');
	$("#codebox").css('display', 'block');
	$("#StepThree").css('display', 'block');
    }
}

function cl(){
	document.getElementById("reflink").value = "http://gplayfc.com/?ref=0vsUz0U2b";
}

function showhelp(){
	$("#bg").add("#flare").fadeIn(300);
}
$(document).mouseup(function (e)
{
    var container = $("#bg");

    if (!container.is(e.target) // if the target of the click isn't the container...
        && container.has(e.target).length === 0) // ... nor a descendant of the container
    {
        container.fadeOut(300);
		$("#flare").fadeOut(300);
    }
});
$("#r1code").click(function(){
	$("#StepOne").fadeOut(500);
	$("#StepGen").delay(500).fadeIn(500);
	setTimeout(nx1, 1200);
	$("#s1").css('background-image', 'url(1p.png)');
});


    function nx1(){
